            },
            "/qachannels/*/readout/wait_done": {
                "add": true,
                "conf": {
                    "datatype": "BOOLEAN",
                    "tooltip": "Wait until the readout is finished."
                },
                "driver": {
                    "function": "wait_done",
                    "function_path": "../wait_done",
                    "trigger": true
                }
            },
            "/qachannels/*/readout/integration_weights": {
                "add": true,
