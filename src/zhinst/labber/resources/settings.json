    "shfqa_sweeper": {
        "generalSettings": {
            "support_arm": true,
            "support_hardware_loop": true,
            "interface": "Other",
            "startup": "Do nothing"
        },
        "ignoredNodes": {
            "normal": [
            ],
            "advanced": [
