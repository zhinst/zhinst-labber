            },
            "/qachannels/*/spectroscopy/wait_done": {
                "add": true,
                "conf": {
                    "datatype": "BOOLEAN",
                    "tooltip": "Wait until the spectroscopy is finished."
                },
                "driver": {
                    "function": "wait_done",
                    "function_path": "../wait_done",
                    "trigger": true
                }
            },
            "/scopes/*/enable": {
                "add": true,
                "conf": {
                    "datatype": "BOOLEAN"
                },
                "driver": {
                    "wait_for": true
                }
            },
            "/scopes/*/wait_done": {
                "add": true,
                "conf": {
                    "datatype": "BOOLEAN",
                    "tooltip": "Wait until the scope recording is finished."
                },
                "driver": {
                    "function": "wait_done",
                    "function_path": "../wait_done",
                    "trigger": true
