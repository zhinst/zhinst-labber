        "quants": {
            "/envelope/waveform": {
                "add": true,
                "extend": {
                    "suffix": "File",
                    "datatype": "PATH",
                    "set_cmd": ".csv",
                    "label": "File",
                    "tooltip": "Complex waveform from a file.",
                    "permission": "WRITE"
                },
                "conf": {
                    "datatype": "VECTOR_COMPLEX"
                }
            },
