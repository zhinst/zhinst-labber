                "driver": {
                    "function": "generator/write_integration_weights",
                    "function_path": "../write_integration_weights",
                    "type": "CSV",
                    "call_empty": false
                }
            },
            "/qachannels/*/spectroscopy/result/enable": {
                "add": true,
                "conf": {
                    "datatype": "BOOLEAN"
                },
                "driver": {
                    "wait_for": true
